<h1>My Cocktails</h1>


<div class="wrapper">
  <ul>
    <% @cocktails.each do |cocktail| %>
      <li>
        <%= cocktail.id %> |
        <%= cocktail.name %>
        <%= link_to 'See details', cocktail_path(cocktail.id)%>
      </li>
    <% end %>
  </ul>

  <p><%= link_to 'Add a Cocktail', new_cocktail_path %></p>
</div>
